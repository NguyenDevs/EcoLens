<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ShapeAppearanceOverlay.FAB.Round" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">40%</item>
    </style>
    <style name="BottomNavigationViewCustom" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="android:textSize">10sp</item>
        <item name="itemIconSize">24dp</item>
        <item name="labelVisibilityMode">labeled</item>
        <item name="android:paddingTop">5dp</item>
    </style>
    <style name="ShapeAppearanceOverlay.App.CornerSize12" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">12dp</item>
    </style>

    <style name="TaxonomyLabelStyle">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_secondary</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">12dp</item>
    </style>

    <style name="TaxonomyValueStyle">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_primary</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">12dp</item>
    </style>

    <style name="ShapeAppearance.App.HomeTree" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>

</resources>